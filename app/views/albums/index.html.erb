<p id="notice"><%= notice %></p>

<h1>Albums</h1>


<% @albums.each do |al| %>
<div class="jumbotron">
<div class="row">
  <h2><%= al.title %> <small>(<%= al.year %>)</small></h2>
    <div class="col-md-6">
      <%= image_tag al.albumart.url(:medium) %>   </div>
        <% al.songs.each do |song| %>
          <div class="col-md-6">
            <h4><%= song.title %> <small><%= song.length %></small></h4> </div>
        <% end %>
</div>
Download it <a href="https://sealab.bandcamp.com/">here.</a> </div>
<% end %>
